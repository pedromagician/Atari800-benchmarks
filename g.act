PROC TESTG()
  BYTE RTCLOK19=19, RTCLOK20=20, CH=0
  CARD I,J
  CARD SCR=88
  GRAPHICS(0)
  RTCLOK20=0
  RTCLOK19=0
  RTCLOK20=0

  CH=0
  FOR I=0 TO 39
  DO
    FOR J=0 TO 23
    DO
      POKE(SCR+(J*40+I),CH)
      CH=CH+1
      IF CH>255 THEN CH=0 FI
    OD
  OD
  GRAPHICS(0)
  PRINTF("%ETime: %U-%U jiffies",RTCLOK19, RTCLOK20)
RETURN
