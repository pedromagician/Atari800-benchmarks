10 GRAPHICS 0
15 POKE 20,0 :POKE 19,0:POKE 20,0
20 SCR=PEEK(88)+256*PEEK(89)
25 CH=0
30 FOR I=40 TO 1 STEP -1
35  FOR L=1 TO 24
40   POKE SCR+(L*40-I),CH
45   CH=CH+1
50   IF CH>255 THEN CH=0
55  NEXT L
60 NEXT I
85 GRAPHICS 0
90 PRINT PEEK(19)
95 PRINT PEEK(20)
990 POKE 764,255
999 IF PEEK(764)=255 THEN 999
